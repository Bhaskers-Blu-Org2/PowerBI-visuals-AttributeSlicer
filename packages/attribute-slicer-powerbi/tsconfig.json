{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "experimentalDecorators": true,
    "jsx": "react",
    "declaration": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true
  },
  "compileOnSave": false,
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
    "power-bi",
    "node_modules",
    "build",
    "typings/browser.d.ts",
    "typings/browser"
  ]
}
